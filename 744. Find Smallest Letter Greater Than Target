class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        char res=letters[0];
        int n=letters.size();
        for(int i=0;i<n;i++){
            if(target<letters[i]){
                res=letters[i];
                break;
            }
        }
        return res;
    }
};
