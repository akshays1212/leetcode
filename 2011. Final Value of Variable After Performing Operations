class Solution {
public:
    int finalValueAfterOperations(vector<string>& operations) {
        int res=0;
        int n=operations.size();
        for(int i=0;i<n;i++){
            if(operations[i]=="X--" or operations[i]=="--X" ){
                res--;
            }
            else{
                res++;
            }
        }
        return res;
    }
};
